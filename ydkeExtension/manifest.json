{
	"manifest_version": 2,
	"name": "YDKe Deck Parser",
	"version": "1.1",
	"description": "Parse YDKe URLs into deck lists.",
	"permissions": [],
	"background": {
	  "scripts": ["background.js"],
	  "persistent": false
	},
	"browser_action": {
	  "default_popup": "popup.html",
	  "default_icon": "icon.png"
	},
	"icons": {
	  "16": "icon16.png",
	  "48": "icon48.png",
	  "128": "icon128.png"
	}
  }
  